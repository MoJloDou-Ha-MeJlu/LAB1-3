<Window x:Class="WpfHello.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:WpfHello"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">
    <Canvas>
        <Button Name="button" Content="Set Name" HorizontalAlignment="Left" Margin="3,5,0,0" VerticalAlignment="Top" Height="36" Width="65" IsCancel="True" Click="Button_Click"/>
        <Button Name="button1" Content="Ret Name" HorizontalAlignment="Left" Margin="3,57,0,0" VerticalAlignment="Top" Height="37" Width="65" IsCancel="True" Click="Button_Click_1"/>
        <TextBox Name="textBox" HorizontalAlignment="Left" Margin="73,5,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="188" Height="36" AutomationProperties.Name="" TextChanged="textBox_TextChanged"/>
        <Label Name="label" Content="" HorizontalAlignment="Left" Margin="73,57,0,0" VerticalAlignment="Top" Height="37" Width="297"/>
        <Button Canvas.Left="3" Canvas.Top="109" Content="Click Me" Height="29" Name="button3" Width="65" Click="button3_Click"/>
        <TextBox Width="188" Canvas.Left="73" Canvas.Top="109" Name="myTextBox" Height="29"/>
    </Canvas>
</Window>
